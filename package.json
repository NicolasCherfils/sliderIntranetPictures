{
    "name": "slider",
    "version": "1.0.0",
    "description": "template",
    "default": "index.html",
    "dependencies": {
        "@babel/polyfill": "^7.12.1",
        "@fortawesome/fontawesome-free": "^6.1.1"
    },
    "build:prod": "parcel build assets/js/main.js --dist-dir ./dist --public-url ./",
    "devDependencies": {
        "@parcel/transformer-sass": "^2.5.0",
        "autoprefixer": "^10.4.7",
        "purgecss": "^4.1.3"
    },
    "scripts": {
        "start": "parcel",
        "build": "parcel build assets/js/main.js --dist-dir ./dist --public-url ./",
        "purgecss": "purgecss --css dist/script.css --content **/*.html dist/script.js -o dist/cleaned/script.css"
    },
    "browserslist": [
        "ie <= 11"
    ]
}